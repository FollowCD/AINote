    "from math import sqrt\n",
    "\n",
    "def fibol(n):\n",
    "    if n==0:\n",
    "        return 0\n",
    "    elif n ==1:\n",
    "        return 1\n",
    "    return fibol(n-1)+fibol(n-2)\n",
    "\n",
    "def fibo2(n):\n",
    "    return ((1+sqrt(5)**n-(1-sqrt(5)))**n)/(2**n*sqrt(5))"
   ]
